{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#ffe9aa",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "template": "\uf0e7"
          },
          "style": "powerline",
          "type": "root"
        },
        {
          "background": "#ffffff",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "template": "{{ .UserName }}"
          },
          "style": "powerline",
          "type": "session"
        },
        {
          "background": "#49c5ff",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "style": "agnoster_short",
            "mapped_locations_enabled": true,
            "max_depth": 1,
            "template": " {{.Path }}"
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "#40fc59",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "template": " {{ .HEAD }} ",
            "fetch_status": true
          },
          "style": "powerline",
          "type": "git"
        },
        {
          "background": "#ff8080",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "template": " \ue20f "
          },
          "style": "powerline",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "background": "#8800dd",
          "foreground": "#ffffff",
          "powerline_symbol": "",
          "invert_powerline": true,
          "properties": {
            "style": "austin",
            "template": " <#fefefe>\ufbab</> {{ .FormattedMs }} ",
            "threshold": 1
          },
          "style": "powerline",
          "type": "executiontime"
        },
        {
          "background": "#906cff",
          "foreground": "#100e23",
          "powerline_symbol": "",
          "invert_powerline": true,
          "properties": {
            "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
            "display_mode": "files"
          },
          "style": "powerline",
          "type": "python"
        },
        {
          "type": "node",
          "style": "powerline",
          "background": "#303030",
          "powerline_symbol": "",
          "invert_powerline": true,
          "properties": {
            "display_mode": "context",
            "fetch_package_manager": true
          }
        },
        {
          "type": "time",
          "style": "powerline",
          "powerline_symbol": "",
          "invert_powerline": true,
          "background": "#40c4ff",
          "foreground": "#ffffff",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#007ACC",
          "properties": {
            "template": "\u276f "
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 1
}